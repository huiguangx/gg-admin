    "@commitlint/config-conventional": "^19.2.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
